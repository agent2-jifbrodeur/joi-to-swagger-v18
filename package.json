{
  "name": "@eval-pairs/joi-to-swagger",
  "version": "7.0.0",
  "description": "Enhanced fork of joi-to-swagger with Joi v18 support and Joi.link() recursive schema handling.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "tap tests.js",
    "test:coverage:html": "tap --coverage-report=html tests.js",
    "test:debug": "tap --node-arg=--inspect --no-timeout tests.js",
    "test:ci": "tap --reporter=tap tests.js",
    "lint": "eslint ./"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "joi",
    "swagger",
    "schema",
    "api",
    "convert",
    "openapi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agent2-jifbrodeur/joi-to-swagger-v18.git"
  },
  "author": "Jocelyn Badgley <joc@twipped.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-twipped": "~3.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "joi": "^18.0.0",
    "@joi/date": "^2.1.0",
    "tap": "^16.3.2",
    "tapsuite": "^2.0.1"
  },
  "peerDependencies": {
    "joi": ">=18.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
